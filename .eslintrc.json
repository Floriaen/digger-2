{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "globals": {
    "dat": "readonly"
  },
  "rules": {
    "no-console": "off",
    "class-methods-use-this": "off",
    "no-underscore-dangle": ["error", { "allowAfterThis": true }],
    "import/extensions": ["error", "always"],
    "max-len": ["error", { "code": 100, "ignoreComments": true }],
    "import/prefer-default-export": "off"
  }
}
